{
  "name": "react-motion-scroll",
  "version": "0.0.4",
  "description": "Smooth scroll as a react-motion plugin",
  "main": "./dist/react-motion-scroll.min.js",
  "scripts": {
    "build": "yarn build:umd & yarn build:example",
    "build:umd": "rollup -c rollup.config.umd.js",
    "build:example": "rollup -c rollup.config.example.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/didierfranc/react-motion-scroll.git"
  },
  "keywords": [
    "react",
    "react-motion",
    "smooth",
    "scroll"
  ],
  "author": "Didier Franc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/didierfranc/react-motion-scroll/issues"
  },
  "homepage": "https://github.com/didierfranc/react-motion-scroll#readme",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-motion": "^0.5.0"
  },
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1"
  }
